{
  "name": "is-svg-fast",
  "version": "1.0.1",
  "license": "WTFPL",
  "description": "Fast check if a string is svg",
  "repository": "farwayer/is-svg-fast",
  "author": "farwayer <farwayer@gmail.com>",
  "keywords": [
    "svg",
    "is-svg"
  ],
  "type": "module",
  "exports": {
    "import": "./index.js",
    "require": "./index.cjs"
  },
  "types": "./index.d.ts",
  "scripts": {
    "build": "esbuild index.js --format=cjs --outfile=index.cjs",
    "publish": "size-limit && yarn build && clean-publish"
  },
  "dependencies": {
    "@rgrove/parse-xml": "^4.1.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "clean-publish": "^4.1.1",
    "esbuild": "^0.17.11",
    "size-limit": "^8.2.4"
  },
  "size-limit": [
    {
      "path": "index.js",
      "limit": "4.8kB"
    }
  ],
  "clean-publish": {
    "fields": ["scripts", "packageManager"]
  },
  "packageManager": "yarn@3.4.1"
}
